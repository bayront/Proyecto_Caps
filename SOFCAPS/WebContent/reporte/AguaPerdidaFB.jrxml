	<parameter name="userC" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
(SELECT SUM(ROUND(consumoActual,2)) FROM listabomba 
	where fechaLecturaActual between  $P{f1}  and  $P{f2} ) AS consumo_bomba,

(SELECT SUM(ROUND(consumoTotal,2)) FROM consumo_reporte 
	where fecha_fin between  $P{f1}  and  $P{f2} ) AS consumo_clientes,
((SELECT SUM(ROUND(consumoActual,2)) FROM listabomba 
	where fechaLecturaActual between  $P{f1}  and  $P{f2} ) - 
(SELECT SUM(ROUND(consumoTotal,2)) FROM consumo_reporte 
	where fecha_fin between  $P{f1}  and  $P{f2} )) AS agua_perdida
FROM listabomba JOIN consumo_reporte GROUP BY consumo_bomba;
]]>
