		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="informes"/>
		<queryString language="SQL">
			<![CDATA[SELECT
(SELECT SUM(ROUND(consumoActual,2)) FROM listabomba 
	where fechaLecturaActual between '2017-02-01' and '2017-04-30') AS consumo_bomba,

(SELECT SUM(ROUND(consumoTotal,2)) FROM consumo_reporte 
	where fecha_fin between '2017-04-01' and '2017-04-30') AS consumo_clientes,
((SELECT SUM(ROUND(consumoActual,2)) FROM listabomba 
	where fechaLecturaActual between '2017-02-01' and '2017-04-30') - 
(SELECT SUM(ROUND(consumoTotal,2)) FROM consumo_reporte 
	where fecha_fin between '2017-04-01' and '2017-04-30')) AS agua_perdida
FROM listabomba JOIN consumo_reporte GROUP BY consumo_bomba;
]]>
		</queryString>
		<field name="consumo_bomba" class="java.lang.Double"/>
		<field name="consumo_clientes" class="java.lang.Double"/>
		<field name="agua_perdida" class="java.lang.Double"/>
	</subDataset>
