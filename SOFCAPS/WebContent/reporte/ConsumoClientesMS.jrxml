	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="recibo"/>
	<parameter name="mini" class="java.lang.String"/>
	<parameter name="aini" class="java.lang.String"/>
	<parameter name="nomini" class="java.lang.String"/>
	<parameter name="userC" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
	CONCAT(cliente.nombre1,' ',cliente.nombre2,' ',cliente.apellido1,' ',cliente.apellido2) AS nombre_completo,
	contrato.numMedidor AS numMedidor,
	consumo.lectura_Actual AS lectura_mes,
	consumo.consumoTotal AS consumo_mes,
	 ROUND((SUM(factura_detalle.consumo * factura_detalle.monto)+factura_maestra.deslizamiento),2) AS totalPago,
	 sector.nombreSector AS sector,
	 date_format(consumo.fecha_fin,'%d/%m/%Y') AS fecha_mes
FROM
	(consumo
	INNER JOIN contrato ON (consumo.Contrato_ID = contrato.Contrato_ID)
	INNER JOIN cliente ON (contrato.Cliente_ID = cliente.Cliente_ID)
	INNER JOIN sector ON (contrato.Sector_ID = sector.Sector_ID)
	INNER JOIN factura_maestra ON (consumo.Consumo_ID = factura_maestra.Consumo_ID)
	INNER JOIN factura_detalle ON (factura_maestra.Factura_Maestra_ID = factura_detalle.Factura_Maestra_ID))
Where
	sector.nombreSector= $P{nomini} and
	consumo.fecha_fin between  $P{mini}   and   $P{aini} 
	and consumo.eliminado=0 and contrato.estado=0 and cliente.estado=0 and consumo.lectura_Actual!=0
GROUP BY
	((((((cliente.nombre1 + ' ') + cliente.nombre2) + ' ') + cliente.apellido1) + ' ') + cliente.apellido2) ,
	contrato.numMedidor, consumo.lectura_Actual,  
	consumo.consumoTotal, sector.nombreSector , consumo.fecha_fin
ORDER BY sector.Sector_ID, nombre_completo, consumo.fecha_fin;]]>
	</queryString>
