		<queryString language="SQL">
			<![CDATA[select o.fecha_fin, o.consumoTotal, o.lectura_Actual, o.Consumo_ID, 
e.Cliente_ID, concat(e.nombre1,' ', e.nombre2,' ', e.apellido1, ' ',e.apellido2) as nombreCompleto, c.Contrato_ID, c. numContrato, c.numMedidor
,o.eliminado,e.estado,c.estado
from consumo o 
inner join cliente e on o.Cliente_ID=e.Cliente_ID
inner join contrato c on c.Contrato_ID=o.Contrato_ID where o.eliminado=0 and e.estado=0 and c.estado=0]]>
		</queryString>
